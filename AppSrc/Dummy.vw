Use Windows.pkg
Use DFClient.pkg
Use cSplitterContainer.pkg
Use cDbScrollingContainer.pkg
Use cDbSplitterContainer.pkg
Use dfEnRad.pkg

Activate_View Activate_oDummy for oDummy
Object oDummy is a dbView

    Set Border_Style to Border_Thick
    Set Size to 207 300
    Set Location to 2 2
    Set Label to "Dummy"
    Set pbAutoActivate to True

            
    Object oDbSplitterContainer1 is a cDbSplitterContainer
        Set piSplitterLocation to 150
        Set pbSplitVertical to False
        Object oDbScrollingContainer1 is a cDbScrollingContainer
            Object oDbScrollingClientArea1 is a cDbScrollingClientArea
            End_Object
        End_Object
        
        Object oDbSplitterContainerChild1 is a cDbSplitterContainerChild
            Object oDbGroup1 is a dbGroup
                Set Size to 122 135
                Set Location to 4 9
                Set Label to 'oDbGroup1'
                Set peAnchors to anTopBottom
            End_Object

            Object oDbGroup2 is a dbGroup
                Set Size to 56 140
                Set Location to 6 155
                Set Label to 'oDbGroup2'
                Set peAnchors to anTopRight
            End_Object

            Object oDbGroup3 is a dbGroup
                Set Size to 59 140
                Set Location to 66 155
                Set Label to 'oDbGroup3'
                Set peAnchors to anTopRight
            End_Object
        End_Object

        Object oDbSplitterContainerChild2 is a cDbSplitterContainerChild
            Object oDbRadioGroup1 is a dbRadioGroup
                Set Location to 14 43
            
                Set Size to 54 232
                Set Label to 'oDbRadioGroup1'
            
                Object oRadio1 is a Radio
                    Set Label to "Radio Label 1"
                    Set Size to 10 61
                    Set Location to 10 5
                End_Object
            
                Object oRadio2 is a Radio
                    Set Label to "Radio Label 2"
                    Set Size to 10 61
                    Set Location to 25 5
                End_Object
            
                Object oRadio3 is a Radio
                    Set Label to "Radio Label 3"
                    Set Size to 10 61
                    Set Location to 40 5
                End_Object
            
            End_Object
        End_Object
    End_Object
        

