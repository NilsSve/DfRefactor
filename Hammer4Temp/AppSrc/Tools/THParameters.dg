                            If (item_count(oTranslate(Self))) Gt 0 Begin
                                Set current_item of (oTranslate(Self)) to 0
                                Set select_state of (oTranslate(Self)) item 0 to True
                            End
                            Delegate Send NotifyChange
                        End
                    End_Procedure // OnClick
                End_Object

