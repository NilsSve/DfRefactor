                            Procedure Key Integer iKeyValue Returns Integer
                                Integer iRetval
                                String  sShortCut
                                Integer[] Keys
                                tShortCut NewShortCut
                                Move "" to sShortCut
                                Get ReadKeyboardState   of oSciCommandHotKeys to Keys
